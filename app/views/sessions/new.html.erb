<h1>Signin Form</h1>

<%= form_for @user, url: sessions_path do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password %>
<!-- <%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %> -->
<%= f.submit "Sign In" %>
<% end %>
