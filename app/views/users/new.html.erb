<h1>Signup Form</h1>

<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :nausea %>
<%= f.number_field :nausea %>
<%= f.label :happiness %>
<%= f.number_field :happiness %>
<%= f.label :tickets %>
<%= f.number_field :tickets %>
<%= f.label :height %>
<%= f.number_field :height %>
Are you an admin?
<%= f.check_box :admin %>
<%= f.submit %>
<% end %>
